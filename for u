<script>
	// força as animações independende o overflow
	new WOW({
		scrollContainer: '.is-wrapper',
		offset: 50}).init();
	new WOW({
		scrollContainer: null,
		offset: 50,
	}).init();

	new WOW({
		scrollContainer: 'body',
		offset: 50,
	}).init();
	
</script>

<script>
	// força os sliders a ficarem com apenas 1 visualização caso o mobile esteja travado
	window.addEventListener('load', () => {
		if(teste_arr) {
			teste_arr?.forEach(slider => {
				if(apenasMobile === 1) {
					slider.glide.update({
						perView: 1,
					});
				}else {
					// mesmo salvando o perview errado no mobile ele irá colocar o correto aqui
					slider.glide.update({
						perView: slider.perView,
					});
				}
			});
		}
	});
</script>

<!-- parallax dos blocos -->
<script>
	const scene = document.querySelectorAll('.is-section');

	if(window.Parallax) {
		scene.forEach(secao => {
			const parallaxInstance = new Parallax(secao);
		});
	}
</script>		<!--	0.01452898979187	-->
